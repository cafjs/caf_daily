    "license": "Apache-2.0",
  "dependencies": {
      "caf_components" : "^0.4.0",
      "caf_transport" : "^0.4.0",
      "node-fetch": "^2.6.0"
  },
    "devDependencies": {
        "eslint": "^6.8.0",
